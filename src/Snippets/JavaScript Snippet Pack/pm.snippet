<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <Header>
        <Title>prompt</Title>
        <Author>Mads Kristensen</Author>
        <Shortcut>pm</Shortcut>
        <Description>Code snippet for "prompt"</Description>
        <SnippetTypes>
            <SnippetType>Expansion</SnippetType>
        </SnippetTypes>
    </Header>
    <Snippet>
        <Declarations>
            <Literal>
                <ID>message</ID>
                <ToolTip>The message</ToolTip>
                <Default>message</Default>
            </Literal>
        </Declarations>
        <Code Language="JavaScript">
            <![CDATA[prompt($message$);$end$]]>
        </Code>
    </Snippet>
</CodeSnippet>