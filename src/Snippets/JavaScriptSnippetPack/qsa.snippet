<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <Header>
        <Title>querySelectorAll</Title>
        <Shortcut>qsa</Shortcut>
        <Author>Mads Kristensen</Author>
        <Description>Code snippet for "querySelectorAll"</Description>
        <SnippetTypes>
            <SnippetType>Expansion</SnippetType>
        </SnippetTypes>
    </Header>
    <Snippet>
        <Declarations>
            <Literal>
                <ID>object</ID>
                <ToolTip>Parent element</ToolTip>
                <Default>document</Default>
            </Literal>
            <Literal>
                <ID>selector</ID>
                <ToolTip>The selector</ToolTip>
                <Default>selector</Default>
            </Literal>
        </Declarations>
        <Code Language="JavaScript">
            <![CDATA[$object$.querySelectorAll('$selector$');$end$]]>
        </Code>
    </Snippet>
</CodeSnippet>